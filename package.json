{
  "name": "@llamaindex/issue-classifier",
  "version": "0.1.0",
  "description": "Classify GitHub Issues as `good first issue` using LlamaExtract",
  "type": "module",
  "bin": {
    "issue-classifier": "dist/index.js"
  },
  "keywords": [
    "github",
    "issues",
    "open-source-management",
    "ai"
  ],
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/bun": "latest",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "prettier": "3.8.1",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@llamaindex/llama-cloud": "^1.5.0",
    "octokit": "^5.0.5",
    "p-limit": "^7.3.0",
    "tslog": "^4.10.2",
    "zod": "^4.3.6"
  },
  "scripts": {
    "test": "vitest run --testTimeout=60000",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
